{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "outDir": "../public/js/app"
  },
  "filesGlob": [
    "./**/*.ts",
    "!./node_modules/**/*.ts",
    "!./public/**"
  ],
  "exclude": [
    "node_modules"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "files": [
    "./assets/app/app.component.ts",
    "./assets/app/app.module.ts",
    "./assets/app/app.routing.ts",
    "./assets/app/components/authorized/dashboard/dashboard.component.ts",
    "./assets/app/components/authorized/dashboard/index.ts",
    "./assets/app/components/frontface/about/about.component.ts",
    "./assets/app/components/frontface/about/index.ts",
    "./assets/app/components/frontface/contact/contact.component.ts",
    "./assets/app/components/frontface/contact/index.ts",
    "./assets/app/components/frontface/home/home.component.ts",
    "./assets/app/components/frontface/home/index.ts",
    "./assets/app/components/frontface/login/index.ts",
    "./assets/app/components/frontface/login/login.component.ts",
    "./assets/app/components/frontface/register/index.ts",
    "./assets/app/components/frontface/register/register.component.ts",
    "./assets/app/components/shared/alert/alert.component.ts",
    "./assets/app/components/shared/alert/index.ts",
    "./assets/app/guards/auth.guard.ts",
    "./assets/app/guards/index.ts",
    "./assets/app/main.ts",
    "./assets/app/models/index.ts",
    "./assets/app/models/user.ts",
    "./assets/app/services/alert.service.ts",
    "./assets/app/services/authentication.service.ts",
    "./assets/app/services/index.ts",
    "./assets/app/services/user.service.ts",
    "./typings.d.ts",
    "./typings/browser.d.ts",
    "./typings/browser/ambient/es6-shim/index.d.ts",
    "./typings/index.d.ts",
    "./typings/main.d.ts",
    "./typings/main/ambient/es6-shim/index.d.ts"
  ]
}
